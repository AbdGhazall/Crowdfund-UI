@using UI.Models.KYC
@model AnnualIncomeRange

<h2 class="mb-5 mt-5">Delete Annual Income Range</h2>
<div>
    <h3 class="mb-3 mt-3">Are you sure you want to delete this?</h3>
    <p><strong>Range:</strong> @Model.Range</p>
    <p><strong>Arabic Range:</strong> @Model.ArabicRange</p>
    @using (Html.BeginForm("Delete", "AnnualIncomeRanges", FormMethod.Post))
    {
        @Html.AntiForgeryToken()
        @Html.HiddenFor(m => m.Id)
        <button type="submit" class="btn btn-danger">Delete</button>
        <a href="@Url.Action("Index")" class="btn btn-secondary">Cancel</a>
    }
</div>

