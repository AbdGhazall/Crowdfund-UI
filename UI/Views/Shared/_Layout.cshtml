                                <li><a class="dropdown-item" href="@Url.Action("Index", "AnnualIncomeRanges")">Annual Income Ranges</a></li>
                                <li><a class="dropdown-item" href="@Url.Action("Index", "Banks")">Banks</a></li>
                                <li><a class="dropdown-item" href="@Url.Action("Index", "Countries")">Countries</a></li>
                                <li><a class="dropdown-item" href="@Url.Action("Index", "Declarations")">Declarations</a></li>
                                <li><a class="dropdown-item" href="@Url.Action("Index", "DocumentTypes")">Document Types</a></li>
                                <li><a class="dropdown-item" href="@Url.Action("Index", "EducationLevels")">Education Levels</a></li>
                                <li><a class="dropdown-item" href="@Url.Action("Index", "EmploymentStatus")">Employment Status</a></li>
                                <li><a class="dropdown-item" href="@Url.Action("Index", "NetWorthRanges")">Net Worth Ranges</a></li>
                                <li><a class="dropdown-item" href="@Url.Action("Index", "RiskAgreements")">Risk Agreements</a></li>
                                <li><a class="dropdown-item" href="@Url.Action("Index", "SocialStatuses")">Social Statuses</a></li>
                                <li><a class="dropdown-item" href="@Url.Action("Index", "SourceOfIncomes")">Source Of Incomes</a></li>
                                <li><a class="dropdown-item" href="@Url.Action("Index", "SourceOfWealthes")">Source Of Wealthes</a></li>
                                <li><a class="dropdown-item" href="@Url.Action("Index", "InvestorTypeSetup")">Investor Type Setup</a></li>
                                <li><a class="dropdown-item" href="@Url.Action("Index", "Questionnaires")">Questionnaires</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    @* <footer class="border-top footer text-muted"> *@
    @*     <div class="container"> *@
    @*         &copy; 2025 - Croudfund Frontend *@
    @*     </div> *@
    @* </footer> *@
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
